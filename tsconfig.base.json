{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": ["ES2022"],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"inlineSources": true,
		"inlineSourceMap": false,
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"types": ["node"]
	}
}
